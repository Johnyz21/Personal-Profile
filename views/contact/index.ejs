<h1 class="text-center">Contact</h1>

<div class="row">
  <div class="col-2">

  </div>
  <div class="col" id="contact_form">
    <form >
      <div class="form-group">
        <label for="name" class="col-form-label">Name</label>
        <input type="text" class="form-control" placeholder="Name" aria-describedby="name">
      </div>
      <div class="form-group">
        <label for="email">Email Address</label>
        <input type="email" class="form-control" placeholder="Email Address" aria-describedby="email">
      </div>
      <div class="form-group">
        <label for="subject">Subject</label>
        <input type="text" class="form-control" placeholder="Subject" aria-describedby="subject">
      </div>
      <div class="form-group">
        <label for="textArea">Message</label>
        <textarea class="form-control"  rows="3"></textarea>
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>

  </div>
  <div class="col-2">

  </div>
</div>
