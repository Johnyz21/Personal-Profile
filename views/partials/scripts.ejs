<script>
  Vue.use(VeeValidate);

  new Vue({
    el: '#submission_form',
    data: {
      name:'',
      email: '',
      message: ''
    },
    methods: {
      validateBeforeSubmit(e) {
        e.preventDefault();
        this.$validator.validateAll().then((result) => {
          if (result) {

            swal({
                title: 'Thanks for getting in touch!',
                text: 'Enjoy your day',
                icon: "success",
                timer: 2500,
                button: {
                  text: "close",
                  closeModal: true,
                  visible: true,
                }
              }).then(function(){
                document.querySelector('#myForm').submit();
              });
          } else {
            swal('Something strange happened', 'Please double check the form!', 'error');
            return false;
          }


        })
      }
    }

  });
</script>


</script>


